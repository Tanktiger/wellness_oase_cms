<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset={{ _charset }}"/>
    <meta name="robots" content="noindex,nofollow" />
    <title>{% block title %}{% endblock %}</title>
    <link rel="stylesheet" href = "{{ asset('bundles/wo/semantic/css/semantic.min.css') }}" />
    <link rel="stylesheet" href = "{{ asset('bundles/wo/css/main.css') }}" />
    {% block head %}{% endblock %}
</head>
<body>

{% block body %}{% endblock %}

{% block javascripts %}
    <script src="{{ asset('bundles/wo/jquery/jquery-2.1.3.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/wo/semantic/js/semantic.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/wo/tinymce/tinymce.min.js') }}" type="text/javascript"></script>
    <script src="{{ asset('bundles/wo/js/main.js') }}" type="text/javascript"></script>
{% endblock %}

</body>
</html>

